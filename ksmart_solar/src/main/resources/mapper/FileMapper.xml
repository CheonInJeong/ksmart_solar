<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace = "com.cafe24.kangk0269.mapper.FileMapper">
	<resultMap type="FileDTO" id="fileResultMap">
		<result property="fileName" column ="file_name"/>
		<result property="fileRealName" column ="file_real_name"/>
		<result property="mId" column ="m_id"/>
		<result property="fileRegDate" column ="file_reg_date"/>
	</resultMap>
	
	<insert id="addFile" parameterType = "list">
		INSERT INTO 
			tb_file(
				file_name, file_save_name, m_id, file_reg_date
			)
		VALUES 
			<foreach collection="list" item="file" separator=".">
			
			(#{file.fileName}, #{file.fileSaveName}, #{file.mId}, now())
	
			</foreach>
	</insert>
	
	<select id="" parameterType="String" resultType="fileDTO">
	
	</select>
</mapper>